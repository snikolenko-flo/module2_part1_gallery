{
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.2",
    "typescript": "^4.7.2"
  },
  "type": "module",
  "name": "module2_part1",
  "description": "",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --fix --ext .ts",
    "build:backend": "npx tsc -p back/tsconfig.json",
    "build:frontend": "npx tsc -p front/tsconfig.json",
    "mv:backend": "mv ./back/backend ./built",
    "mv:frontend": "mv front/frontend ./built",
    "cp:backend": "cp -a front/src/html/. front/src/css/. ./built",
    "cp:frontend": "cp -a back/src/images ./built",
    "build": "mkdir ./built && run-s build:backend build:frontend mv:backend mv:frontend cp:backend cp:frontend",
    "prettier_write": " npx prettier --write .",
    "prettier_check": "npx prettier --check .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon ./built/backend/app.js",
    "watch": "npx tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snikolenko-flo/module2_part1_gallery.git"
  },
